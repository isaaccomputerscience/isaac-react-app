    "@types/jest": "^27.4.1",
    "@types/js-cookie": "^3.0.4",
    "@types/katex": "^0.16.3",
    "@types/leaflet": "^1.9.6",
    "@types/lodash": "^4.14.199",
    "@types/object-hash": "^3.0.4",
    "@types/qrcode": "^1.5.2",
    "@types/react-beautiful-dnd": "^13.1.5",
